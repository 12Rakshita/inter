#include <stdio.h>

int main() {
    char input;
    printf("Enter a character (A, B, or C): ");
    scanf(" %c", &input); 

    switch (input) {
        case 'A':
            printf("Symbol: &&\n");
            break;
        case 'B':
            printf("Symbol: ||\n");
            break;
        case 'C':
            printf("Symbol: ^\n");
            break;
        default:
            printf("Invalid input! Please enter A, B, or C.\n");
            break;
    }

    return 0;
}
